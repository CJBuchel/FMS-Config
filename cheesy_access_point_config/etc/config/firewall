config defaults
	option syn_flood '1'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'REJECT'

config zone
	option name 'lan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	option network 'lan vlan100'

# Define FRC Teams Networks

config zone
	option name 'teams'
	option forward 'REJECT'
	option input 'REJECT'
	option output 'REJECT'
	option network 'vlan10 vlan20 vlan30 vlan40 vlan50 vlan60 vlan70 vlan80 vlan90'

config include
	option path '/etc/firewall.user'

#FRC Teams Rules

config rule
	option name 'teams-input-icmp'
	option src 'teams'
	option proto 'icmp'
	option family 'ipv4'
	option target 'ACCEPT'

config rule
	option name 'teams-output-icmp'
	option dest 'teams'
	option proto 'icmp'
	option family 'ipv4'
	option target 'ACCEPT'
    
config rule
	option name 'teams-forward-icmp'
	option src 'teams'
    	option dest 'teams'
	option proto 'icmp'
	option family 'ipv4'
    	option target 'ACCEPT'

