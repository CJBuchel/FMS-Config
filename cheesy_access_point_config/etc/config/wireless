
config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11g'
	option path 'soc/soc:pcie-controller/pci0000:00/0000:00:02.0/0000:02:00.0'
	option htmode 'HT20'
	option disabled '0'
	option txpower '20'
	option country 'US'
	option channel '11'

config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option hidden '1'
	option encryption 'psk2+ccmp'
	option network 'vlan100'
	option ssid 'Cheesy Arena'
	option macaddr '62:38:e0:12:6b:17'
	option key '1234Five'

config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'soc/soc:pcie-controller/pci0000:00/0000:00:01.0/0000:01:00.0'
	option txpower '23'
	option country 'US'
	option htmode 'HT20'
	option basic_rates '6500,7200'
	option short_gi_20 '0'
	option channel '157'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option isolate '0'
	option bgscan '0'
	option wds '0'
	option maxassoc '1'
	option hidden '1'
	option network 'vlan10'
	option encryption 'psk2+ccmp'
	option disabled '0'
	option ssid '1'
	option key '11111111'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option isolate '0'
	option bgscan '0'
	option wds '0'
	option maxassoc '1'
	option hidden '1'
	option network 'vlan20'
	option encryption 'psk2+ccmp'
	option disabled '0'
	option ssid '2'
	option key '22222222'

